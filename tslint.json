{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "no-console": false,
        "variable-name": [
            true,
            "allow-pascal-case",
            "allow-leading-underscore",
            "allow-snake-case",
            "allow-trailing-underscore",
            "require-const-for-all-caps"
          ]
    },
    "rulesDirectory": []
}